<% if user_signed_in? && post.liked_by?(current_user)%>
  <%=link_to "いいね済", post_likes_path(post.id),  method: :delete, class: "post-like-delete" %>
<% elsif user_signed_in? && ! post.liked_by?(current_user) %>
  <%=link_to "いいねする", post_likes_path(post.id), method: :post, class: "post-like-create" %>
<% end %>